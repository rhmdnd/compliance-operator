{
  "extends": [
    "config:recommended"
  ],
  "prConcurrentLimit": 30,
  "labels": [
    "ok-to-test",
    "px-approved",
    "docs-approved",
    "qe-approved"
  ],
  "schedule": [
    "every weekend"
  ],
  "gomod": {
    "packageRules": [
      {
        "matchManagers": ["gomod"],
        "matchBaseBranches": ["/^release-.*/"],
        "enabled": false
      }
    ]
  }
}
